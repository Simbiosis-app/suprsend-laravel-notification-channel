{
    "name": "laravel-notification-channels/suprsend",
    "description": "Enables SuprSend as a notification channel for Laravel applications",
    "homepage": "https://github.com/laravel-notification-channels/suprsend",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Rivadeneyra",
            "email": "marcorivm@gmail.com",
            "homepage": "https://marcorivm.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0 || ~9.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0 || ~9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SuprSend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SuprSend\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
